
@{
    ViewData["Title"] = "Details";
}

<!doctype html>

<html lang="de">
<head>
    <title></title>
    <meta charset="utf-8" />
</head>
<body>
    <header>
        <h2>Departments</h2>
    </header>

    <table class="table">
        <tr>
            <th>Nachname</th>
            <th>Vorname</th>
            <th>Job</th>
        </tr>
        @foreach (var employee in Model.EmployeeList)
        {
            <tr>
                <td title="Klicken Sie hier um auf das Profil zu gelangen"><a asp-controller="Employees" asp-action="Profil" asp-route-employeeId="@employee.Id">@employee.Lastname</a></td>
                <td title="Klicken Sie hier um auf das Profil zu gelangen"><a asp-controller="Employees" asp-action="Profil" asp-route-employeeId="@employee.Id">@employee.Firstname</a></td>
                <td>@employee.Job</td>
            </tr>
        }
    </table>
</body>
</html>