@model ProjectManager.Core.Entities.Qualification

@{
    ViewData["Title"] = "Delete";
}

<div class="container">
    <div class="row">
        <div class="col-lg-9">
            <div class="panel panel-default">
                <div class="panel-body">
                    <div class="page-header">
                        <h3>Do you want to delete the Qualification "@Model.QualificationName" ?</h3>
                    </div>
                    <div>
                        <form asp-action="DeleteConfirm" asp-route-qualId="@Model.Id">
                            <input type="submit" value="Delete" class="btn btn-danger" />
                            <a asp-action="List" class="btn btn-default">Overview</a>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


