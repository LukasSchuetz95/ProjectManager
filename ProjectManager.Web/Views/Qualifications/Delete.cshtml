@model ProjectManager.Core.Entities.Qualification

@{
    ViewData["Title"] = "Delete";
}

<h3>Möchten Sie die Qualifikation "@Model.QualificationName" wirklich löschen ?</h3>
<div>
    <hr />
    <form asp-action="DeleteConfirm" asp-route-qualId="@Model.Id">
        <input type="submit" value="Löschen" class="btn btn-danger" /> 
        <a asp-action="List" class="btn btn-default">Zur Übersicht</a>
    </form>
</div>
