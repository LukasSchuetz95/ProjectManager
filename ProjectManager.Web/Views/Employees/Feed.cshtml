@model ProjectManager.Web.Models.ViewModel.Employees.EmployeesFeedViewModel

@{
    ViewData["Title"] = "EditView";
}

<!doctype html>

<html lang="de">
<head>
    <title></title>
    <meta charset="utf-8" />
</head>
<body>
    <div id="header">
        <h1>Der Feed</h1>
        <p>Your personal Overview</p>
    </div>
    <table class="table">
        <tr>
            <th>Projects</th>
        </tr>
        @foreach (var obj in Model.EmployeeProject)
        {
            <tr>
                <td>@obj.Project</td>               
            </tr>
        }
    </table>
</body>
</html>

